{
  "presets": ["modern-browsers"],
  "plugins": [
    "babel-plugin-add-module-exports",
    "@babel/plugin-syntax-logical-assignment-operators"
  ],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "modules": "commonjs"
        }]
      ]
    }
  },
  "overrides": [
    {
      "test": "./src/lib/*.js",
      "sourceType": "script",
      "presets": [
         ["@babel/preset-env", {
           "modules": "commonjs"
         }]
      ]
    }
  ]
}